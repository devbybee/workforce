			<url>http://www.asyst.co.id/licenses/ASYST-LICENSE-1.0.txt</url>
			<distribution>repo</distribution>
			<comments>Asyst Proprietary license</comments>
		</license>
	</licenses>

	<organization>
		<name>PT Aero Systems Indonesia</name>
		<url>http://www.asyst.co.id/</url>
	</organization>

	<developers>
